{
    "model": {
        "EMA": {
            "decay": 0.999
        },
        "fixmatch": {
            "factor": 1.0,
            "temperature": 1.0,
            "threshold": 0.95
        }
    },
    "dataset": {
        "name": "ssl_cifar10",
        "root": "data/cifar10",
        "num_labeled": 4000,
        "batch_size": {
            "labeled": 64,
            "unlabeled": 448
        }
    },
    "optim": {
        "optimizer": {
            "name": "SGD",
            "lr": 0.03,
            "momentum": 0.9,
            "weight_decay": 0.0005,
            "nesterov": true
        },
        "scheduler": {
            "T_max": 1048576,
            "T_warm": 0
        },
        "interval": "step"
    },
    "trainer": {
        "max_steps": 1048576
    }
}