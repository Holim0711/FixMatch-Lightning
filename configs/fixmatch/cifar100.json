{
    "method": "fixmatch",
    "transform": {
        "weak": [
            {"name": "RandomHorizontalFlip"},
            {"name": "RandomCrop", "size": 32, "padding": 4, "padding_mode": "reflect"},
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR100"}
        ],
        "str": [
            {"name": "RandomHorizontalFlip"},
            {"name": "RandomCrop", "size": 32, "padding": 4, "padding_mode": "reflect"},
            {"name": "RandAugmentUDA", "n": 2, "fillcolor": "CIFAR100"},
            {"name": "Cutout", "ratio": 0.5, "fillcolor": "CIFAR100"},
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR100"}
        ],
        "val": [
            {"name": "ToTensor"},
            {"name": "Normalize", "dataset": "CIFAR100"}
        ]
    },
    "model": {
        "backbone": {
            "src": "weaver",
            "name": "wide_resnet28_8",
            "num_classes": 100
        },
        "loss_u": {
            "temperature": 1.0,
            "threshold": 0.95
        },
        "ema": 0.999,
        "lrelu": 0.1
    },
    "dataset": {
        "name": "CIFAR100",
        "num_labeled": 400,
        "random_seed": 0,
        "batch_sizes": {
            "labeled": 64,
            "unlabeled": 448,
            "val": 512
        }
    },
    "optimizer": {
        "name": "SGD",
        "lr": 0.03,
        "momentum": 0.9,
        "weight_decay": 0.001,
        "nesterov": true
    },
    "scheduler": {
        "name": "HalfCosineAnnealingLR",
        "T_max": 10700
    }
}